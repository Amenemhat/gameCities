generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model Sessions {
  id             Int     @default(autoincrement()) @id
  ChatID         Float   @unique
  spentCities    Json?
  lastLetter     String?
  scoreInSession Int?
}

model botContext {
  id              Int    @default(autoincrement()) @id
  ChatID          Float  @unique
  translate       String
  text            String
  sessions        Sessions
  userName        String
  highScore       Int
}