generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model Sessions {
  id             Int     @default(autoincrement()) @id
  ChatID         Int  @unique
  spentCities    Json?
  lastLetter     String?
  scoreInSession Int?

  @@map("sessions")
}

model botContext {
  id              Int    @default(autoincrement()) @id
  ChatID          Int  @unique
  translate       String
  text            String
  sessions        Sessions
  userName        String
  highScore       Int

  @@map("botContext")
}