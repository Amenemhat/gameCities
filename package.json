  },
  "scripts": {
    "start": "node --inspect src/bot.js",
    "debug": "node --nolazy --inspect-brk=9229 src/bot.js",
    "eslintinit": "node_modules/.bin/eslint --init",
    "eslint": "node_modules/.bin/eslint --fix",
